<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="mybatis.Reserve_mybatis">	
	<insert id="insertReserve" parameterType="java.util.Map">
		insert into reserve values('RC'||seq_reserve.nextval, #{screening_code}, #{id}, #{people}, #{seat}, #{seatnumber}, sysdate)
	</insert>
</mapper>